{
  "name": "phoenix-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Phoenix Legacy Planning Platform",
  "type": "module",
  "scripts": {
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend dev",
    "build:apps": "pnpm --filter \"./apps/**\" build",
    "dev": "vite",
    "dev:respectful": "VITE_RESPECTFUL_ONBOARDING=true vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:error-logging": "node scripts/test-error-logging.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:will": "vitest will",
    "migrate": "tsx scripts/run-migrations.ts",
    "migrate:prod": "NODE_ENV=production tsx scripts/run-migrations.ts",
    "backup": "tsx scripts/backup-wills.ts",
    "backup:prod": "NODE_ENV=production tsx scripts/backup-wills.ts",
    "docker:build": "docker build -t legacyguard/phoenix:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "npm run build && npm run docker:build && npm run docker:push",
    "test:ci": "vitest run --reporter=verbose --watch=false --coverage=false",
    "check:cycles": "node scripts/ci/cycles-guard.cjs",
    "check:exports": "node scripts/ci/exports-guard.cjs",
    "check:all": "npm run -s check:cycles && npm run -s check:exports && npm run -s check:size && npm run -s check:only && npm run -s check:console && npm run -s check:imports && npm run -s check:img && npm run -s check:init && npm run -s check:secrets",
    "ci": "npm run -s lint && npm run -s typecheck && npm run -s test:ci && npm run -s check:all",
    "check:size": "node scripts/ci/size-guard.cjs",
    "check:size:strict": "node scripts/ci/size-guard.cjs --strict",
    "size:baseline": "node scripts/ci/size-guard.cjs --write-baseline",
    "check:only": "node scripts/ci/only-guard.cjs",
    "check:console": "node scripts/ci/console-guard.cjs",
    "check:console:strict": "node scripts/ci/console-guard.cjs --strict",
    "console:baseline": "node scripts/ci/console-guard.cjs --write-baseline",
    "typecheck:strict": "tsc -p tsconfig.strict.json --noEmit",
    "typecheck:strict-plus": "tsc -p tsconfig.strict-plus.json --noEmit",
    "check:imports": "node scripts/ci/imports-guard.cjs",
    "build:pwa": "vite build --config vite.config.pwa.ts",
    "check:img": "node scripts/ci/img-guard.cjs",
    "check:img:strict": "node scripts/ci/img-guard.cjs --strict",
    "img:baseline": "node scripts/ci/img-guard.cjs --write-baseline",
    "img:autofix": "node scripts/tools/img-autofix.cjs",
    "check:img:priority": "node scripts/ci/img-priority-guard.cjs",
    "build:critical": "vite build --config vite.config.critical.ts",
    "check:init": "node scripts/ci/init-chunk-guard.cjs",
    "init:baseline": "node scripts/ci/init-chunk-guard.cjs --write-baseline",
    "check:init:strict": "node scripts/ci/init-chunk-guard.cjs --strict",
    "check:secrets": "node scripts/ci/secrets-guard.cjs",
    "secrets:baseline": "node scripts/ci/secrets-guard.cjs --write-baseline",
    "check:secrets:strict": "node scripts/ci/secrets-guard.cjs --strict",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --browser electron --headless",
    "build:e2e": "vite -c vite.config.e2e.shim.ts build",
    "preview:e2e": "vite preview --port 4173 --strictPort",
    "e2e:run:smoke": "cypress run --browser electron --headless --spec \"cypress/e2e/smoke/*.cy.ts\""
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.42.1",
    "@growthbook/growthbook-react": "^1.6.1",
    "@hookform/resolvers": "^3.10.0",
