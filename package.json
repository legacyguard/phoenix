    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "^6.30.1",
    "react-shepherd": "^6.1.9",
    "recharts": "^2.15.4",
    "shepherd.js": "^14.5.1",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^2.6.0",
